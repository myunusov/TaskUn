<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        >
    <bean id="traceAdvice" class="org.maxur.commons.advice.TraceAdvice" factory-method="aspectOf">
        <property name="notifier" ref="notifierImpl"/>
    </bean>

    <bean id="benchmarkAdvice" class="org.maxur.commons.advice.BenchmarkAdvice" factory-method="aspectOf">
        <property name="notifier" ref="notifierImpl"/>
    </bean>

</beans>
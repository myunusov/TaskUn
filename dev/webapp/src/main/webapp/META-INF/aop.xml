 <!DOCTYPE aspectj PUBLIC
        "-//AspectJ//DTD//EN" "http://www.eclipse.org/aspectj/dtd/aspectj.dtd">
<aspectj>

    <weaver options="-XnoInline -verbose -showWeaveInfo -Xlint:ignore -Xset:weaveJavaxPackages=true">
        <include within="javax.*"/>
        <include within="org.aspectj.*"/>
        <include within="org.maxur..*"/>
    </weaver>

    <aspects>
        <aspect name="org.maxur.commons.advice.BenchmarkAdvice"/>
        <aspect name="org.maxur.commons.advice.TraceAdvice"/>
    </aspects>

</aspectj>